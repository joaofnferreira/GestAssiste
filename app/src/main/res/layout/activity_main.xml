<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F2F4"
    android:backgroundTint="#9ED8DD"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/criarassistencia"
        android:layout_width="182dp"
        android:layout_height="92dp"
        android:layout_marginStart="4dp"

        android:layout_marginTop="124dp"

        android:text="Criar Assistencia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/consultaassiste"
        android:layout_width="172dp"
        android:layout_height="91dp"
        android:layout_marginStart="208dp"

        android:layout_marginTop="124dp"
        android:text="Consultar Assistencia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="299dp"
        android:layout_height="283dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="152dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/assiste" />

</androidx.constraintlayout.widget.ConstraintLayout>