<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F2F4"
    android:backgroundTint="#9ED8DD"
    tools:context=".fragmentos.Clientes">


    <EditText
        android:id="@+id/cemail"
        android:layout_width="226dp"
        android:layout_height="61dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="256dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ctelemovel"
        android:layout_width="217dp"
        android:layout_height="45dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="188dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/cnome"
        android:layout_width="219dp"
        android:layout_height="38dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:inputType="textPersonName"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nomecliente"
        android:layout_width="49dp"
        android:layout_height="16dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="140dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/emailcliente"
        android:layout_width="49dp"
        android:layout_height="15dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="292dp"
        android:text="Email :"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/telemovelcliente"
        android:layout_width="72dp"
        android:layout_height="18dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="204dp"
        android:text="Telemovel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

